<h2>Sign In Here</h2>
<%= form_for(resource, as: resource_name, url: user_session_path(resource_name)) do |f| %>
  <%= f.text_field :email, placeholder: "Enter email", class: "form-control" %><br>
  <%= f.password_field :password, placeholder: "Password", class: "form-control" %> <br>
  <%= f.submit class: "btn btn-primary" %>
 <% end %>

<br></br>
<p>Need to sign up? <%=link_to "Create an account", new_user_registration_path %>
